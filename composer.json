{
    "name": "fh/http",
    "description": "Fh http client interface",
    "keywords": ["http client"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dan Martin",
            "email": "dmartin@fh.org"
        }
    ],
    "require": {
        "php": ">=5.3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7.27"
    },
    "suggest": {
      "kriswallsmith/buzz": "HTTP client for Buzz implementation",
      "guzzle/guzzle": "HTTP client for the Guzzle implementation"
    },
    "autoload": {
        "psr-4": {
            "Fh\\Http\\": "src/"
         }
    },
    "autoload-dev": {
        "psr-4": {
            "Fh\\Http\\Test\\": "tests/"
         }
    },
    "minimum-stability": "dev"
}
